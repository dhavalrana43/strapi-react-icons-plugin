  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dhavalrana43/strapi-react-icons-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/dhavalrana43/strapi-react-icons-plugin/issues"
  },
  "homepage": "https://github.com/dhavalrana43/strapi-react-icons-plugin#readme",
  "author": "Dhaval Rana <drana@plusonex.com>"
}
